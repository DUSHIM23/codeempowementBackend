{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "FeaturedImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "PublishDate": {
      "type": "datetime"
    },
    "Shortsummary": {
      "type": "text"
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "Categories": {
      "type": "enumeration",
      "enum": [
        "Tech",
        "Youth Empowerment",
        "Education",
        "events",
        "insights",
        "impact",
        "news",
        "success stories"
      ],
      "default": "Youth Empowerment",
      "required": true
    },
    "Tags": {
      "type": "enumeration",
      "enum": [
        "We will add them"
      ]
    },
    "excerpt": {
      "type": "string"
    },
    "readTime": {
      "type": "string",
      "default": "5 min read"
    },
    "teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team.team"
    }
  }
}
